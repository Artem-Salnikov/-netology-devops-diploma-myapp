#!groovy

pipeline {
	agent any
  environment {
    DOCKERHUB_CREDENTIALS = credentials('dockerhub')
  }
  stages {
    stage('Get last tag') {
      steps {
        script {
          def output = sh(returnStdout: true, script: 'git tag --list --sort=-version:refname \"0*\" | head -n 1')
          echo "Output: ${output}"
      	//sh "last_tag=\$(git tag --list --sort=-version:refname \"0*\" | head -n 1)"
      }
    }
    }
  }
    stage('Dockerhub login') {
      steps {
        sh "echo \$output"
  }
    }
  }
 
